{
    "name": "lsflk/unique-uid",
    "description": "Unique  User id generator",
    "keywords": [
        "lsflk",
        "unique-uid"
    ],
    "homepage": "https://github.com/lsflk/unique-uid",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Nizar",
            "email": "nizarucsc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lsf\\UniqueUid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lsf\\UniqueUid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
